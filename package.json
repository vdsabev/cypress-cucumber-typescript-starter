{
  "name": "cypress-cucumber-typescript-starter",
  "version": "1.0.0",
  "description": "A structured template for using Cypress with Cucumber and TypeScript",
  "scripts": {
    "start": "cypress open",
    "test": "cypress run --spec \"**/*.feature\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vdsabev/cypress-cucumber-typescript-starter.git"
  },
  "keywords": [
    "cypress",
    "cucumber",
    "automation",
    "testing"
  ],
  "author": "Vladimir Sabev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vdsabev/cypress-cucumber-typescript-starter/issues"
  },
  "homepage": "https://github.com/vdsabev/cypress-cucumber-typescript-starter#readme",
  "dependencies": {},
  "devDependencies": {
    "cypress": "6.1.0",
    "cypress-cucumber-preprocessor": "4.0.0",
    "typescript": "4.1.2"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": "cypress/integration",
    "nonGlobalStepBaseDir": "cypress/integration",
    "commonPath": "common"
  }
}
